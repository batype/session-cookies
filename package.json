{
  "name": "session-cookies",
  "private": false,
  "version": "0.1.3",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "default": "./lib/index.d.ts"
      },
      "browser": {
        "default": "./lib/session-cookies.js"
      },
      "default": {
        "default": "./lib/index.js"
      }
    }
  },
  "author": "songshao",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "lib"
  ],
  "unpkg": "lib/session-cookies.js",
  "scripts": {
    "build": "tsc && node build.js",
    "prepublish": "yarn build",
    "publish": "npm run release && npm run build && npm publish",
    "release": "np --any-branch --no-yarn --no-tests --no-cleanup  --contents",
    "test": "jest --config=jest.config.json"
  },
  "keywords": [
    "cookie",
    "cookies",
    "session",
    "sessionStorage",
    "localStorage",
    "sesssion-cookie"
  ],
  "devDependencies": {
    "@sindresorhus/tsconfig": "0.7.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "jest": "26.6.3",
    "np": "^7.6.0",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5"
  },
  "xo": {
    "extends": [
      "prettier"
    ]
  },
  "lint-staged": {
    "src/*.js": [
      "npm run test",
      "prettier --semi false --single-quote --write",
      "git add"
    ]
  },
  "license": "MIT",
  "__npminstall_done": true,
  "dependencies": {
    "esbuild": "^0.18.17",
    "tsc": "^2.0.4"
  },
  "homepage": "https://gitee.com/songsshao_admin/session-cookies/blob/master/README.md",
  "repository": "https://gitee.com/songsshao_admin/session-cookies.git"
